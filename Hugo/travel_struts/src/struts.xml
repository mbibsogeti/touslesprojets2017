<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
	"http://struts.apache.org/dtds/struts-2.5.dtd">

<!-- START SNIPPET: xworkSample -->
<struts>

	<!-- Some or all of these can be flipped to true for debugging -->
	<constant name="struts.i18n.reload" value="true" />
	<constant name="struts.custom.i18n.resources" value="global" />
	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.devMode" value="true" />
	<constant name="struts.configuration.xml.reload" value="true" />

	<package name="welcome" namespace="/" extends="struts-default">
		<default-action-ref name="index" />
		<action name="index">
			<result>/WEB-INF/views/index.jsp</result>
		</action>
	</package>

	<package name="ireland" namespace="/Ireland" extends="struts-default">
		<action name="index">
			<result>/WEB-INF/views/Ireland/index.jsp</result>
		</action>
		<action name="info">
			<result>/WEB-INF/views/Ireland/info.jsp</result>
		</action>
		<action name="viajes" class="fr.ib.hugo.travel.ireland.TravelsAction">
			<result>/WEB-INF/views/Ireland/viajes.jsp</result>
		</action>
	</package>

	<package name="england" namespace="/England" extends="struts-default">
		<action name="index" class="fr.ib.hugo.travel.england.EnglandInfo">
			<result>/WEB-INF/views/England/index.jsp</result>
		</action>
		<action name="registration" class="fr.ib.hugo.travel.england.EnglandRegistration">
			<result>/WEB-INF/views/England/registration.jsp</result>
		</action>
		<action name="send" class="fr.ib.hugo.travel.england.SendAction">
			<result name="success">/WEB-INF/views/England/send.jsp</result>
			<result name="input">/WEB-INF/views/England/registration.jsp</result>
		</action>		
	</package>

</struts>

<!-- END SNIPPET: xworkSample -->

