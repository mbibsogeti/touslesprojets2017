<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="obodrel.fr.android_travel_obodrel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/obodrel_img_ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".activity.HomeActivity"
            android:label="@string/activity_home">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"/>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MapActivity"
            android:label="@string/activity_main">
        </activity>
        <activity
            android:name=".activity.RegisterActivity"
            android:label="@string/activity_register">
        </activity>
        <activity
            android:name=".activity.SubMapsActivity"
            android:label="@string/activity_west">
        </activity>
        <activity
            android:name=".activity.PlaneActivity"
            android:icon="@drawable/airplane_64"
            android:label="@string/activity_plane">
        </activity>
    </application>

</manifest>
