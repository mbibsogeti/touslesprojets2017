<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd" xmlns:p="tapestry:parameter" t:type="AsiaLayout" title="China" pageName="china">
		<div class="col-xs-12">
			<h3>${message:welcome_message}</h3>
			<t:beandisplay object="travel">
			</t:beandisplay>
			<t:form>
				<t:textfield t:id="name" t:mixins="formgroup" validate="required,minlength=3"/>
				<t:textfield t:id="telephoneNumber" t:mixins="formgroup" validate="required,minlength=9,regexp=^[+]?[\d\s]+$"/>
				<t:textfield type="number" t:id="travellers" t:mixins="formgroup" validate="required,min=1,max=10"/>				
				<t:upload t:id="visa" t:mixins="formgroup" validate="required"/>
				<t:submit class="btn btn-primary"/>
				<t:errors/>
			</t:form>
			<a href="#" t:type="eventlink" t:event="switchLocale" t:context='literal:en'>British</a>
			<a href="#" t:type="eventlink" t:event="switchLocale" t:context='literal:fr'>French</a>
		</div>
</html>