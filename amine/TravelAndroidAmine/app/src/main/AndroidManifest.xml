<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="fr.ib.travelandroidamine">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name=".HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"/>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity android:name=".MapActivity" android:label="@string/oceanie"></activity>
        <activity android:name=".SubmapsActivity" android:icon="@drawable/icon" android:label="@string/details">
        </activity>
        <activity android:name=".PlaneActivity" android:label="@string/plane">
        </activity>
        <activity android:name=".RegisterActivity" android:label="@string/register_title"></activity>


    </application>

</manifest>
