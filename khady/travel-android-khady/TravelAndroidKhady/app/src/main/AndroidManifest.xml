<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="fr.ib.travelandroidkhady">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconmipmap"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <activity android:name=".HomeActivity"
            android:label="@string/home">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"/>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>


        <activity android:name=".RegisterActivity"
            android:label="@string/register"/>

        <activity android:name=".MapActivity"
            android:label="@string/gen_map"/>


        <activity android:name=".SubmapsActivity"
            android:label="@string/specif_map"
        android:icon="@drawable/icon"/>


        <activity android:name=".PlaneActivity"
            android:label="@string/plane"/>


    </application>

</manifest>
